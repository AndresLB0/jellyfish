<?php

class Marca extends validator 
{
    private $idmarca = null;
    private $nombre= null;
    private $Modelo = null;

    public function setidmarca($value)
    {
        if ($this->validateNaturalNumber($value)){
            $this->idmarca = $value;
            return true;
        } else{
           return false;
        }
    }

    public function setNombre($value)
    {
        $set = (nombre);
        if (setNombre(Nombre=?)) {
            return true;
        } else {
            return false;
        }
    }

    public function setModelo($value)
    {
        if ($this->setModelo($value)){
            $this->idModelo = $value;
            return true;
        } else{
           return false;
        }
    }



    public function getIdMarca()
    {
        return $this->idMarca;
    }

    public function getNombre()
    {
        return $this->setNombre;
    }

    public function getIdModelo()
    {
        return $this->setModelo;
    }
